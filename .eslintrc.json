{
    "plugins": [
        "prettier"
    ],
    "extends": [
        "eslint:recommended", 
        "next/core-web-vitals", 
        "prettier"
    ],
    "rules": {
        "no-case-declarations": "off",
        "no-empty-function": "off",
        "prefer-const": "off",
        "prettier/prettier": "error",
        "@next/next/no-img-element": "off",
        "react/no-unescaped-entities":"off"
    }
}
